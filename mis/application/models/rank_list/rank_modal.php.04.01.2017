       private function getOtherStuRes($admn_no,$sem,$etype){
           
           $q="select * from final_semwise_marks_foil where admn_no='$admn_no' and semester='$sem' and `type`=(select `type` from final_semwise_marks_foil where admn_no='$admn_no' and semester='$sem' and `type`<>'R' order by `session_yr`,`type` desc limit 1 )" ;
           $r=$this->db->query($q);
            if($r->num_rows() >0){
               $re= $r->row();
               if($re->type == 'O'){
                  $etype++;
                   $re->type=$etype;
               }else if($re->type=='O'){
                   $etype++; $etype++;
                   $re->type=etype;
               }
               return $re;
           }
           return false;
           
       }
	   

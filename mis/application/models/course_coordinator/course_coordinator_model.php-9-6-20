      function UpdateGrades($data,$gradeval){
		  
		// up 1 grade for session winter and session year 2019-20 start
		
		if($gradeval=="F"){
          $gradeval="D";
        }elseif($gradeval=="D"){
          $gradeval="C";
        }elseif($gradeval=="C"){
          $gradeval="C+";
        }elseif($gradeval=="C+"){
          $gradeval="B";
        }elseif($gradeval=="B"){
          $gradeval="B+";
        }elseif($gradeval=="B+"){
          $gradeval="A";
        }elseif($gradeval=="A"){
          $gradeval="A+";
        }
		
		
		// end  
		  
        $updateVal=array(
          "grade"=>$gradeval
        );
        $this->db->where($data);
        $this->db->update('cbcs_marks_subject_description', $updateVal);
      }
